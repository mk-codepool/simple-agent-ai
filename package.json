{
  "name": "ai-web-user",
  "version": "0.1.0",
  "private": true,
  "description": "TypeScript Express REST API with hot reload",
  "main": "dist/index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development ts-node-dev --respawn --transpile-only --poll --ignore-watch node_modules --no-notify src/index.ts",
    "build": "rimraf dist && tsc -p tsconfig.json",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "lint:fix": "eslint \"src/**/*.{ts,tsx}\" --fix",
    "format": "prettier --check .",
    "format:write": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "start:mcp:playwright": "mcp-server-playwright --port 8931 --isolated"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@openai/agents": "^0.0.8",
    "@playwright/mcp": "^0.0.34",
    "cors": "*",
    "dotenv": "*",
    "express": "*",
    "helmet": "*",
    "zod": "3.25.67"
  },
  "devDependencies": {
    "@types/cors": "*",
    "@types/express": "*",
    "@types/helmet": "*",
    "@types/node": "*",
    "@typescript-eslint/eslint-plugin": "*",
    "@typescript-eslint/parser": "*",
    "cross-env": "*",
    "eslint": "*",
    "eslint-config-airbnb-base": "*",
    "eslint-config-airbnb-typescript": "*",
    "eslint-config-prettier": "*",
    "eslint-import-resolver-typescript": "*",
    "eslint-plugin-import": "*",
    "eslint-plugin-prettier": "*",
    "playwright": "^1.55.0",
    "prettier": "*",
    "rimraf": "*",
    "ts-node-dev": "*",
    "typescript": "*"
  }
}
